<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ProStudy</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.protostar.prostudy.protostarAdmin.services.ProtostarAdminService,com.protostar.prostudy.protostarAdmin.services.UploadURLService,com.protostar.prostudy.service.AttendanceService,com.protostar.prostudy.service.BookService,com.protostar.prostudy.service.CertificateService,com.protostar.prostudy.service.ChapterService,com.protostar.prostudy.service.DivisionService,com.protostar.prostudy.service.InstituteService,com.protostar.prostudy.service.PartnerSchoolService,com.protostar.prostudy.service.PaymentService,com.protostar.prostudy.service.PracticeExamResultService,com.protostar.prostudy.service.PracticeExamService,com.protostar.prostudy.service.QuestionService,com.protostar.prostudy.service.ScheduleStudentExamService,com.protostar.prostudy.service.ScheduledExamResultService,com.protostar.prostudy.service.ScheduledExamService,com.protostar.prostudy.service.ScheduledQuestionService,com.protostar.prostudy.service.StandardService,com.protostar.prostudy.service.StudSubService,com.protostar.prostudy.service.StudentService,com.protostar.prostudy.service.SubjectService,com.protostar.prostudy.service.SyllabusService,com.protostar.prostudy.service.TestService,com.protostar.prostudy.service.UploadURLService,com.protostar.prostudy.service.UserService</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>ObjectifyFilter</filter-name>
    <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ObjectifyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>
        com.protostar.prostudy.until.AppServletContextListener
    </listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>UploadLogoServlet</display-name>
    <servlet-name>UploadLogoServlet</servlet-name>
    <servlet-class>com.protostar.prostudy.protostarAdmin.services.UploadLogoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadLogoServlet</servlet-name>
    <url-pattern>/UploadLogoServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <description></description>
    <display-name>BlobDocServe</display-name>
    <servlet-name>BlobDocServe</servlet-name>
    <servlet-class>com.protostar.prostudy.until.BlobDocServe</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BlobDocServe</servlet-name>
    <url-pattern>/serve</url-pattern>
  </servlet-mapping>
</web-app>