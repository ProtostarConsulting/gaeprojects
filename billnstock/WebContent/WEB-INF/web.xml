<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="WebApp_ID" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>BillnStock</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.protostar.billingnstock.account.services.AccountEntityService,com.protostar.billingnstock.account.services.AccountEntryService,com.protostar.billingnstock.account.services.AccountGroupService,com.protostar.billingnstock.account.services.AccountService,com.protostar.billingnstock.account.services.GeneralEntryService,com.protostar.billingnstock.account.services.GeneralJournalService,com.protostar.billingnstock.assetmanagement.services.AssetService,com.protostar.billingnstock.crm.services.CrmService,com.protostar.billingnstock.crm.services.OpportunityService,com.protostar.billingnstock.cust.services.CustomerService,com.protostar.billingnstock.hr.services.HrService,com.protostar.billingnstock.invoice.services.InvoiceService,com.protostar.billingnstock.proadmin.services.ProAdminService,com.protostar.billingnstock.purchase.services.PurchaseOrderService,com.protostar.billingnstock.purchase.services.SupplierService,com.protostar.billingnstock.sales.services.SalesOrderService,com.protostar.billingnstock.setup.services.UploadURLService,com.protostar.billingnstock.setup.services.setupService,com.protostar.billingnstock.stock.services.StockItemService,com.protostar.billingnstock.tax.services.TaxService,com.protostar.billingnstock.user.services.UserService,com.protostar.billingnstock.warehouse.services.WarehouseService,com.protostar.billnstock.service.TestService</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>ObjectifyFilter</filter-name>
    <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ObjectifyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>
        com.protostar.billnstock.until.data.AppServletContextListener
    </listener-class>
  </listener>
  <servlet>
    <description/>
    <display-name>UploadLogo</display-name>
    <servlet-name>UploadLogo</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.UploadLogo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadLogo</servlet-name>
    <url-pattern>/UploadLogo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description/>
    <display-name>Serve</display-name>
    <servlet-name>Serve</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.Serve</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Serve</servlet-name>
    <url-pattern>/serve</url-pattern>
  </servlet-mapping>
  <servlet>
    <description/>
    <display-name>UploadFooter</display-name>
    <servlet-name>UploadFooter</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.UploadFooter</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadFooter</servlet-name>
    <url-pattern>/UploadFooter</url-pattern>
  </servlet-mapping>
  <servlet>
    <description/>
    <display-name>UploadExcelSheetAddUsers</display-name>
    <servlet-name>UploadExcelSheetAddUsers</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.UploadExcelSheetAddUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadExcelSheetAddUsers</servlet-name>
    <url-pattern>/UploadExcelSheetAddUsers</url-pattern>
  </servlet-mapping>
  <servlet>
    <description/>
    <display-name>ExcelStockUpload</display-name>
    <servlet-name>ExcelStockUpload</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.ExcelStockUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExcelStockUpload</servlet-name>
    <url-pattern>/ExcelStockUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <description/>
    <display-name>ExcelCustomerUpload</display-name>
    <servlet-name>ExcelCustomerUpload</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.ExcelCustomerUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExcelCustomerUpload</servlet-name>
    <url-pattern>/ExcelCustomerUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <description/>
    <display-name>ExcelAccountsUpload</display-name>
    <servlet-name>ExcelAccountsUpload</servlet-name>
    <servlet-class>com.protostar.billingnstock.document.ExcelAccountsUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExcelAccountsUpload</servlet-name>
    <url-pattern>/ExcelAccountsUpload</url-pattern>
  </servlet-mapping>
</web-app>